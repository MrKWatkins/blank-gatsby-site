{
    "name": "blank-gatsby-site",
    "description": "A blank Gatsby site to use as a template when creating a new site. Comes preconfigured to be written in TypeScript and SASS with support for Markdown and BlueprintJS and testing with Jest and Enzyme.",
    "version": "1.0.0",
    "scripts": {
        "build": "node_modules/.bin/babel-node --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby build",
        "build:debug": "node_modules/.bin/babel-node --inspect-brk --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby build",
        "develop": "node_modules/.bin/babel-node --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby develop",
        "develop:debug": "node_modules/.bin/babel-node --inspect-brk --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby develop",
        "start": "node_modules/.bin/babel-node --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby develop",
        "test": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.11.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-modules-commonjs": "^7.2.0",
        "@babel/plugin-transform-typescript": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-typescript": "^7.1.0",
        "@types/enzyme": "^3.1.15",
        "@types/jest": "^23.3.13",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "babel-plugin-remove-graphql-queries": "^2.5.2",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.23",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-preset-gatsby": "^0.1.6",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "gatsby": "^2.0.91",
        "gatsby-plugin-sass": "^2.0.7",
        "gatsby-plugin-tslint": "^0.0.2",
        "gatsby-plugin-typescript": "^2.0.3",
        "gatsby-plugin-typescript-checker": "^1.0.3",
        "gatsby-source-filesystem": "^2.0.16",
        "gatsby-transformer-remark": "^2.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.6.0",
        "jest-css-modules": "^1.1.0",
        "node-sass": "^4.11.0",
        "react-test-renderer": "^16.7.0",
        "ts-jest": "^23.10.5",
        "tslint": "^5.12.1",
        "tslint-loader": "^3.5.4",
        "tslint-react": "^3.6.0",
        "typescript": "^3.2.4",
        "webpack": "^4.28.4"
    }
}
